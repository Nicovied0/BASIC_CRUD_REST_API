{
  "version": 2,
  "builds": [{ "src": "src/index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/index", "dest": "src/routes/index.js" },
    { "src": "/api/movies", "dest": "src/routes/movies.js" },
    { "src": "/api/users", "dest": "src/routes/users.js" },
    { "src": "/(.*)", "dest": "src/index.js" }
  ]
}
